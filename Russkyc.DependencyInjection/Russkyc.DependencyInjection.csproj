<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <PackageIcon>Resources\Images\Russkyc.png</PackageIcon>
        <Title>Minimal Dependency Injection</Title>
        <Copyright>Russell Camo 2023</Copyright>
        <Authors>Russell Camo</Authors>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Version>2.1.0</Version>
        <LangVersion>8</LangVersion>
        <PackageProjectUrl>https://github.com/russkyc/dependency-injection</PackageProjectUrl>
        <RepositoryUrl>https://github.com/russkyc/dependency-injection</RepositoryUrl>
        <PackageReleaseNotes>v2.X.X:
            Changes:
            - Add service registration by attribute
            - Add multiple assembly support
            
            Fixes:
            - Fixed container injection
            
            Breaking:
            - Removed BuilderServices. See repo for updated setup.</PackageReleaseNotes>
    </PropertyGroup>
    <PropertyGroup>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
    </PropertyGroup>
    <ItemGroup>
        <None Update="Resources\Images\Russkyc.png">
            <Pack>True</Pack>
            <PackagePath></PackagePath>
        </None>
        <None Remove="README.md" />
    </ItemGroup>
</Project>
